 ...formState,\n      [name]: value,\n    });\n  };\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const { data } = await updateStudent({\n        variables: { ...formState },\n      });\n\n      Auth.login(data.updateStudent.token);\n    } catch (e) {\n      console.error(e);\n    }\n  };\n\n  return (\n    <>\n      <Modal show={show} onHide={handleClose} animation={false}>\n        <Modal.Header closeButton>\n          <Modal.Title>Update your Profile</Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n          <form className=\"row\" onSubmit={handleFormSubmit}>\n            <div className=\"col-12\">\n              <label htmlFor=\"validationCustom01\" className=\"form-label\">\n                Surname\n              </label>\n              <input\n                type=\"text\"\n                className=\"form-control\"\n                id=\"validationCustom01\"\n                placeholder=\"Mark\"\n                name=\"firstName\"\n                value={formState.surname}\n                onChange={handleChange}\n                required\n              />\n            </div>\n\n            <div className=\"col-12\">\n              <label htmlFor=\"validationCustom01\" className=\"form-label\">\n                Email address\n              </label>\n              <input\n                type=\"text\"\n                className=\"form-control\"\n                id=\"validationCustom01\"\n                placeholder=\"email address\"\n                name=\"email\"\n                value={formState.email}\n                onChange={handleChange}\n                required\n              />\n            </div>\n            <div className=\"col-12\">\n              <label htmlFor=\"validationCustom01\" className=\"form-label\">\n                Password\n              </label>\n              <input\n                type=\"password\"\n                className=\"form-control\"\n                id=\"validationCustom01\"\n                placeholder=\"\"\n                name=\"password\"\n                value={formState.password}\n                onChange={handleChange}\n              />\n            </div>\n            <div className=\"col-12\">\n              <button\n                className=\"btn btn-primary update-btn-profile\"\n                type=\"submit\"\n              >\n                Update Profile\n              </button>\n            </div>\n          </form>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button variant=\"secondary\" onClick={handleClose}>\n            Cancel\n          </Button>\n        </Modal.Footer>\n      </Modal>\n    </>\n  );\n}\n","/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/utils/mutations.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/GuruProfileList/index.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/PhotoHolder/index.js",["132","133","134"],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/GuruProfileDetails/index.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/PageNotFound/index.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/Header/index.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/styles/Container.style.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/styles/Flex.style.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/styles/Button.style.js",[],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/Footer/index.js",["135"],"/Users/rafaelaferreira/Desktop/She_Gurus_Project3/She_Gurus/client/src/components/Card/index.js",["136","137"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nimport PlaceHol